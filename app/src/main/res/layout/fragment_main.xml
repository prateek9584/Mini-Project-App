<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.prateek.Miniproject.ownerActivity">


   <TextView
       android:id="@+id/textView"
       android:layout_marginTop="200dp"
       android:textColor="#00E5FF"

       android:drawableLeft="@drawable/user_ic"
       android:textSize="30dp"
       android:paddingLeft="1dp"
       android:layout_width="match_parent"
       android:background="@drawable/textedit_background"
       android:layout_height="50dp"
       android:text="PRATEEK" />

   <TextView
       android:id="@+id/textView2"
       android:layout_width="match_parent"
       android:background="@drawable/textedit_background"
       android:layout_marginTop="3dp"
       android:textSize="30dp"
       android:paddingLeft="1dp"
       android:drawableLeft="@drawable/girluser"
       android:textColor="#F50057"
       android:layout_height="50dp"
       android:text="PREETI" />

   <TextView
       android:id="@+id/textView3"
       android:background="@drawable/textedit_background"
       android:drawableLeft="@drawable/user_ic"
       android:textSize="30dp"
       android:layout_marginTop="3dp"
       android:layout_width="match_parent"
       android:textColor="#C6FF00"
       android:layout_height="50dp"
       android:paddingLeft="1dp"
       android:text="PREM" />

   <TextView
       android:layout_marginTop="3dp"
       android:id="@+id/textView4"
       android:textSize="30dp"
       android:layout_width="match_parent"
       android:textColor="#E91E63"
       android:paddingLeft="1dp"
       android:drawableLeft="@drawable/girluser"
       android:background="@drawable/textedit_background"
       android:layout_height="50dp"
       android:text="PRIYA" />

   <TextView
       android:layout_marginTop="3dp"
       android:id="@+id/textView5"
       android:textSize="30dp"
       android:background="@drawable/textedit_background"
       android:paddingLeft="1dp"
       android:drawableLeft="@drawable/user_ic"
       android:layout_width="match_parent"
       android:textColor="#E65100"
       android:layout_height="50dp"
       android:text="RAKESH" />

</LinearLayout>



